<Window x:Class="RPGFront.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RPGFront"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="350">
    <Grid>
        <RadioButton x:Name="MageRadio" Content="Mage" HorizontalAlignment="Right" Margin="0,43,20,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="WarriorRadio" Content="Warrior" HorizontalAlignment="Right" Margin="0,43,100,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="ShamanRadio" Content="Shaman" HorizontalAlignment="Left" Margin="100,43,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="RogueRadio" Content="Rogue" HorizontalAlignment="Left" Margin="20,43,0,0" VerticalAlignment="Top"/>
        <Button x:Name="GenerujButon" Content="Generuj" HorizontalAlignment="Center" Margin="0,100,0,0" VerticalAlignment="Top" Click="GenerujButon_Click"/>

    </Grid>
</Window>
